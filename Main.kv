# File name: main.py
#: kivy 2.0.0


ScreenManagement:
    MainScreen:
    QuickGame:

<QuickGame>:
    name: 'quickgame'
    FloatLayout:
        Button:
            on_release: app.root.current = 'main'
            text: 'Home'
            font_size: self.height / 2
            size_hint: 0.2,0.1
            #pos: root.right-self.width,root.top-self.height
            pos_hint: {'right':1,'top':1}
        Button:
            size_hint_x: 0.6
            size_hint_y: 0.6
            pos_hint: {'right':0.8,'top':0.8}



